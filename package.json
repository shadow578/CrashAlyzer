{
  "name": "@shadow578/crashalyzer",
  "version": "2.0.1",
  "description": "CrashAlyzer is your easy-to-use(â„¢) crashlog analyzer for ARM Cortex-M microcontrollers.",
  "author": "shadow578",
  "license": "MIT",
  "keywords": [
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shadow578/CrashAlyzer.git"
  },
  "homepage": "https://github.com/shadow578/CrashAlyzer",
  "bugs": {
    "url": "https://github.com/shadow578/CrashAlyzer/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "main": "lib/cli.js",
  "types": "lib/index.d.ts",
  "preferGlobal": true,
  "bin": {
    "crashalyzer": "./lib/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run build & node .",
    "test": "jest --config jestconfig.json",
    "test:watch": "jest --config jestconfig.json --watch",
    "format": "prettier --ignore-path .gitignore --write \"src/**/*.+(js|ts|json)\"",
    "format:check": "prettier --ignore-path .gitignore --check \"src/**/*.+(js|ts|json)\"",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext .js,.ts src",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm test && npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.6",
    "@types/yargs": "^17.0.29",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.2",
    "ts-jest": "^29.0.3",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "enquirer": "^2.4.1",
    "readline": "^1.3.0",
    "yargs": "^17.7.2"
  }
}
